{
    "private": true,
    "scripts": {
        "dev": "npm run development",
        "back-dev": "npm run back-development",
        "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "back-development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --env.mixfile=webpack.back.mix --config=node_modules/laravel-mix/setup/webpack.config.js",
